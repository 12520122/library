<Window x:Class="QuanLyThuVien.View.LienKeWeb"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LienKeWeb" Height="214.486" Width="361.682">
    <Grid Margin="0,0,2,1">
        <Grid.Background>
            <ImageBrush ImageSource="/QuanLyThuVien;component/Resources/skyBackground.jpg"/>
        </Grid.Background>
        <Label Content="Liên kết vơi Web thư viên khác" HorizontalAlignment="Left" Margin="64,10,0,0" VerticalAlignment="Top" Width="223" Height="32" FontSize="20" FontFamily="Times New Roman" Foreground="Red"/>
        <ComboBox Name="ListWeb" HorizontalAlignment="Left" Margin="7,67,0,0" VerticalAlignment="Top" Width="332"/>
        <Button x:Name="XacNhan" Content="Xác nhận" HorizontalAlignment="Left" Margin="35,125,0,0" VerticalAlignment="Top" Width="85" Height="31" Template="{DynamicResource ButtonStyle}" Click="XacNhan_Click"/>
        <Button x:Name="Thoat" Content="Thoát" HorizontalAlignment="Left" Margin="212,125,0,0" VerticalAlignment="Top" Width="85" Height="31" Template="{DynamicResource ButtonStyle}" Click="Thoat_Click"/>

    </Grid>
</Window>
